<?xml version="1.0" encoding="UTF-8"?>
<plugin xmlns="http://apache.org/cordova/ns/plugins/1.0" id="dvolland-cordova-plugin-ios-no-export-compliance" version="1.0.2">
    <name>iOS No Export Compliance</name>
    <description>This plugin sets ITSAppUsesNonExemptEncryption to false in *-Info.plist file</description>
    <license>ISC</license>
    <engines>
        <engine name="cordova" version=">=3.0.0" />
    </engines>
    <!-- ios -->
    <platform name="ios">
        <config-file platform="ios" target="*-Info.plist" parent="ITSAppUsesNonExemptEncryption">
          <false/>
        </config-file>
    </platform>
</plugin>
